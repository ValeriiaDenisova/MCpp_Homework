
public class ASCII {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(char i ='A'; i <= 'Z'; i++) {
			System.out.print(i);
		}
		
		System.out.println();
		
		for(char i ='z'; i >= 'a'; i--) {
			System.out.print(i);
		}
		
		System.out.println();
		
		for(char i ='а'; i <= 'я'; i++) {
			System.out.print(i);
		}
		
		System.out.println();
	}

}
